Listen 8080
<VirtualHost *:8080>
    WSGIDaemonProcess zvmsdkwsgi user=zvmsdk group=zvmsdk processes=2 threads=5
        WSGIProcessGroup zvmsdkwsgi

    WSGIScriptAlias / /usr/bin/zvmsdk-wsgi
    <Directory /usr/lib/python3.9/site-packages/zvmsdk/sdkwsgi>
        Require all granted
    </Directory>

    <Directory /usr/bin>
    <Files zvmsdk-wsgi>
        Require all granted
    </Files>
    </Directory>
</VirtualHost>
